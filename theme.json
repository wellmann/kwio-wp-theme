{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "useRootPaddingAwareAlignments": true,
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": null,
      "gradients": [],
      "link": true,
      "palette": [
        {
          "name": "Background Light",
          "slug": "background-light",
          "color": "#FFFFFF"
        },
        {
          "name": "Background Dark",
          "slug": "background-dark",
          "color": "#000000"
        },
        {
          "name": "Background Blue",
          "slug": "background-blue",
          "color": "#0074D9"
        },
        {
          "name": "Text Light",
          "slug": "text-light",
          "color": "#FFFFFF"
        },
        {
          "name": "Text Dark",
          "slug": "text-dark",
          "color": "#000000"
        },
        {
          "name": "Body Background",
          "slug": "body-background",
          "color": "#FFFFFF"
        },
        {
          "name": "Body Text",
          "slug": "body-text",
          "color": "#000000"
        },
        {
          "name": "Body Link",
          "slug": "body-link",
          "color": "#0074D9"
        },
        {
          "name": "Body Accent",
          "slug": "body-accent",
          "color": "#0074D9"
        }
      ]
    },
    "layout": {
      "contentSize": "960px",
      "wideSize": "1024px",
      "allowEditing": false,
      "allowCustomContentAndWideSize": false
    },
    "spacing": {
      "blockGap": true,
      "margin": false,
      "padding": false,
      "customSpacingSize": false,
      "spacingSizes": [
        {
          "name": "100",
          "slug": "100",
          "size": "4rem"
        },
        {
          "name": "200",
          "slug": "200",
          "size": "8rem"
        },
        {
          "name": "300",
          "slug": "300",
          "size": "16rem"
        },
        {
          "name": "400",
          "slug": "400",
          "size": "32rem"
        },
        {
          "name": "500",
          "slug": "500",
          "size": "64rem"
        }
      ]
    },
    "dimensions": {
      "defaultAspectRatios": false,
      "aspectRatios": [
        {
					"name": "4:3",
					"slug": "4-3",
					"ratio": "4/3"
				},
        {
					"name": "16:9",
					"slug": "16-9",
					"ratio": "16/9"
				},
        {
					"name": "9:16",
					"slug": "9-16",
					"ratio": "9/16"
				}
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": false,
      "fontWeight": false,
      "fluid": {
        "minViewportWidth": "360px",
        "maxViewportWidth": "1200px"
      },
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": false,
      "dropCap": false,
      "fontSizes": [
        {
          "name": "14",
          "slug": "14",
          "size": "14px",
          "fluid": {
            "min": "12px",
            "max": "14px"
          }
        },
        {
          "name": "16",
          "slug": "16",
          "size": "16px",
          "fluid": {
            "min": "14px",
            "max": "16px"
          }
        },
        {
          "name": "20",
          "slug": "20",
          "size": "20px",
          "fluid": {
            "min": "17px",
            "max": "20px"
          }
        },
        {
          "name": "26",
          "slug": "26",
          "size": "26px",
          "fluid": {
            "min": "22px",
            "max": "26px"
          }
        },
        {
          "name": "30",
          "slug": "30",
          "size": "30px",
          "fluid": {
            "min": "25px",
            "max": "30px"
          }
        },
        {
          "name": "40",
          "slug": "40",
          "size": "40px",
          "fluid": {
            "min": "34px",
            "max": "40px"
          }
        }
      ],
      "fontFamilies": [
        {
          "name": "Helvetica",
          "slug": "helvetica",
          "fontFamily": "Helvetica, sans-serif"
        },
        {
          "name": "Times",
          "slug": "times",
          "fontFamily": "Times, sans-serif"
        },
        {
          "name": "Consolas",
          "slug": "consolas",
          "fontFamily": "Consolas, sans-serif"
        }
      ]
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": false
        },
        "color": {
          "background": false,
          "text": false
        }
      },
      "core/columns": {
        "spacing": {
          "margin": true,
          "padding": true
        }
      },
      "core/column": {
        "spacing": {
          "padding": true
        }
      },
      "core/group": {
        "spacing": {
          "margin": true
        }
      },
      "core/heading": {
        "color": {
          "text": false,
          "background": false
        }
      },
      "core/paragraph": {
        "color": {
          "text": true,
          "background": false,
          "link": false,
          "palette": [
            {
              "name": "Body Text",
              "slug": "body-text",
              "color": "#000000"
            },
            {
              "name": "Body Link",
              "slug": "body-link",
              "color": "#0074D9"
            }
          ]
        },
        "typography": {
          "fontFamilies": [],
          "fontSizes": [
            {
              "name": "20",
              "slug": "20",
              "size": "20px",
              "fluid": {
                "min": "17px",
                "max": "20px"
              }
            }
          ]
        }
      }
    },
    "custom": {
      "h1": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "40px",
        "line-height": "1.2",
        "color": "#000000"
      },
      "h2": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "30px",
        "line-height": "1.2",
        "color": "#000000"
      },
      "h3": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "26px",
        "line-height": "1.1923076923076923",
        "color": "#000000"
      },
      "h4": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "20px",
        "line-height": "1.2",
        "color": "#000000"
      },
      "p": {
        "font-family": "Helvetica",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "16px",
        "line-height": "1.375",
        "color": "#000000"
      },
      "breadcrumb": {
        "font-family": "Consolas",
        "font-style": "normal",
        "font-weight": "600px",
        "font-size": "14px",
        "line-height": "1.5",
        "color": "#000000"
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--body-background)",
      "text": "var(--wp--preset--color--body-text)"
    },
    "typography": {
      "fontSize": "var(--wp--preset--font-size--20)",
      "fontFamily": "var(--wp--custom--font-family--base)",
      "lineHeight": "1.4"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--custom--h-1--font-size)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--custom--h-2--font-size)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--custom--h-3--font-size)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--custom--h-4--font-size)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--custom--h-5--font-size)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--custom--h-6--font-size)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--body-link)"
        },
        "typography": {
          "textDecoration": "underline"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--body-link-hover)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":active": {
          "color": {
            "text": "var(--wp--custom--color--link)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":visited": {
          "color": {
            "text": "var(--wp--custom--color--link)"
          }
        }
      }
    },
    "spacing": {
      "blockGap": "3rem",
      "padding": {
        "right": "20px",
        "left": "20px"
      }
    }
  }
}