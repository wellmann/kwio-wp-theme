{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "useRootPaddingAwareAlignments": true,
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": null,
      "gradients": [],
      "link": true,
      "palette": [
        {
          "name": "Background Light",
          "slug": "background-light",
          "color": "#FFFFFF"
        },
        {
          "name": "Background Dark",
          "slug": "background-dark",
          "color": "#000000"
        },
        {
          "name": "Background Blue",
          "slug": "background-blue",
          "color": "#0074D9"
        },
        {
          "name": "Text Light",
          "slug": "text-light",
          "color": "#FFFFFF"
        },
        {
          "name": "Text Dark",
          "slug": "text-dark",
          "color": "#000000"
        },
        {
          "name": "Body Background",
          "slug": "body-background",
          "color": "#FFFFFF"
        },
        {
          "name": "Body Text",
          "slug": "body-text",
          "color": "#000000"
        },
        {
          "name": "Body Link",
          "slug": "body-link",
          "color": "#0074D9"
        },
        {
          "name": "Body Accent",
          "slug": "body-accent",
          "color": "#0074D9"
        }
      ]
    },
    "layout": {
      "contentSize": "960px",
      "wideSize": "1024px"
    },
    "spacing": {
      "blockGap": true,
      "margin": false,
      "padding": false,
      "customSpacingSize": false,
      "spacingSizes": [
        {
          "name": "100",
          "slug": "100",
          "size": "4rem"
        },
        {
          "name": "200",
          "slug": "200",
          "size": "8rem"
        },
        {
          "name": "300",
          "slug": "300",
          "size": "16rem"
        },
        {
          "name": "400",
          "slug": "400",
          "size": "32rem"
        },
        {
          "name": "500",
          "slug": "500",
          "size": "64rem"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": false,
      "fontWeight": false,
      "fluid": {
        "minViewportWidth": "360px",
        "maxViewportWidth": "1200px"
      },
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": false,
      "dropCap": false,
      "fontSizes": [
        {
          "name": "100",
          "slug": "100",
          "size": "1rem",
          "fluid": {
            "min": "0.75rem",
            "max": "1rem"
          }
        },
        {
          "name": "200",
          "slug": "200",
          "size": "1.125rem",
          "fluid": {
            "min": "0.875rem",
            "max": "1.125rem"
          }
        },
        {
          "name": "300",
          "slug": "300",
          "size": "1.25rem",
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          }
        },
        {
          "name": "400",
          "slug": "400",
          "size": "1.3125rem",
          "fluid": {
            "min": "1.125rem",
            "max": "1.3125rem"
          }
        },
        {
          "name": "500",
          "slug": "500",
          "size": "2rem",
          "fluid": {
            "min": "1.625rem",
            "max": "2rem"
          }
        },
        {
          "name": "600",
          "slug": "600",
          "size": "2.75rem",
          "fluid": {
            "min": "2.25rem",
            "max": "2.75rem"
          }
        }
      ],
      "fontFamilies": [
        {
          "name": "Base",
          "slug": "base",
          "fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif"
        },
        {
          "name": "Heading",
          "slug": "heading",
          "fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol"
        },
        {
          "name": "Mono",
          "slug": "mono",
          "fontFamily": "Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace"
        }
      ]
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": false
        },
        "color": {
          "background": false,
          "text": false
        }
      },
      "core/columns": {
        "spacing": {
          "margin": true,
          "padding": true
        }
      },
      "core/column": {
        "spacing": {
          "padding": true
        }
      },
      "core/group": {
        "spacing": {
          "margin": true
        }
      },
      "core/paragraph": {
        "color": {
          "background": false
        }
      }
    },
    "custom": {
      "h1": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "40px",
        "line-height": "1.2",
        "color": "#000000"
      },
      "h2": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "30px",
        "line-height": "1.2",
        "color": "#000000"
      },
      "h3": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "26px",
        "line-height": "1.1923076923076923",
        "color": "#000000"
      },
      "h4": {
        "font-family": "Times",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "20px",
        "line-height": "1.2",
        "color": "#000000"
      },
      "p": {
        "font-family": "Helvetica",
        "font-style": "normal",
        "font-weight": "normal",
        "font-size": "16px",
        "line-height": "1.375",
        "color": "#000000"
      },
      "breadcrumb": {
        "font-family": "Consolas",
        "font-style": "normal",
        "font-weight": "600px",
        "font-size": "14px",
        "line-height": "1.5",
        "color": "#000000"
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--body-background)",
      "text": "var(--wp--preset--color--body-text)"
    },
    "typography": {
      "fontFamily": "var(--wp--custom--font-family--base)",
      "lineHeight": "1.4"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--custom--h-1--font-size)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--custom--h-2--font-size)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--custom--h-3--font-size)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--custom--h-4--font-size)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--custom--h-5--font-size)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--custom--h-6--font-size)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--body-link)"
        },
        "typography": {
          "textDecoration": "underline"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--body-link-hover)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":active": {
          "color": {
            "text": "var(--wp--custom--color--link)"
          },
          "typography": {
            "textDecoration": "none"
          }
        },
        ":visited": {
          "color": {
            "text": "var(--wp--custom--color--link)"
          }
        }
      }
    },
    "spacing": {
      "blockGap": "3rem",
      "padding": {
        "right": "20px",
        "left": "20px"
      }
    }
  }
}